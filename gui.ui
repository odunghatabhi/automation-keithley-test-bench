<?xml version='1.0' encoding='utf-8'?>
<interface version="1.3">
  <object class="tk.Toplevel" id="toplevel1">
    <property name="background">#9dceff</property>
    <property name="geometry">480x320</property>
    <property name="height">200</property>
    <property name="resizable">both</property>
    <property name="title" translatable="yes">Multiplexer Control</property>
    <property name="width">200</property>
    <child>
      <object class="tk.Text" id="text3">
        <property name="background">#9dceff</property>
        <property name="borderwidth">0</property>
        <property name="font">{Arial} 10 {bold}</property>
        <property name="height">1</property>
        <property name="relief">flat</property>
        <property name="setgrid">false</property>
        <property name="state">normal</property>
        <property name="text" translatable="yes">MULTIPLEXER No:</property>
        <property name="undo">false</property>
        <property name="width">17</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relx">0.32</property>
          <property name="rely">0.62</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.OptionMenu" id="comport" named="True">
        <property name="command" type="command" cbtype="simple">comval</property>
        <property name="value">COM</property>
        <property name="values">COM5,COM3,COM4,COM31,COM7,Other</property>
        <property name="variable">string:port</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relx">0.05</property>
          <property name="rely">0.16</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Entry" id="comentry" named="True">
        <property name="state">normal</property>
        <property name="textvariable">string:com</property>
        <property name="validatecommand" type="command" cbtype="entry_validate" args="">comenter</property>
        <property name="width">13</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relx">0.22</property>
          <property name="rely">0.18</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="comopen" named="True">
        <property name="command" type="command" cbtype="simple">openport</property>
        <property name="relief">raised</property>
        <property name="state">normal</property>
        <property name="text" translatable="yes">OPEN COM</property>
        <property name="textvariable">string:open</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relx">0.05</property>
          <property name="rely">0.29</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Message" id="status" named="True">
        <property name="background">#a0a0a0</property>
        <property name="borderwidth">0</property>
        <property name="font">{Arial} 10 {}</property>
        <property name="justify">center</property>
        <property name="pady">50</property>
        <property name="text" translatable="yes">COM STATUS</property>
        <property name="textvariable">string:comstatus</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="height">30</property>
          <property name="relheight">0.0</property>
          <property name="relwidth">0.0</property>
          <property name="relx">0.11</property>
          <property name="rely">0.44</property>
          <property name="width">100</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="close" named="True">
        <property name="command" type="command" cbtype="simple">serialclose</property>
        <property name="text" translatable="yes">CLOSE COM</property>
        <property name="textvariable">string:closecom</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relx">0.23</property>
          <property name="rely">0.29</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.OptionMenu" id="optionmenu3">
        <property name="command" type="command" cbtype="simple">selectmux</property>
        <property name="value">-</property>
        <property name="values">1,2,3,4,5,6</property>
        <property name="variable">int:muxnum</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relx">0.58</property>
          <property name="rely">0.6</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="button" named="True">
        <property name="activebackground">#ffffff</property>
        <property name="activeforeground">#ffffff</property>
        <property name="background">#008000</property>
        <property name="command" type="command" cbtype="simple">muxon</property>
        <property name="font">{Arial} 9 {bold}</property>
        <property name="text" translatable="yes">ON</property>
        <property name="textvariable">string:on</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relx">0.53</property>
          <property name="rely">0.71</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="buttoff1" named="True">
        <property name="activebackground">#ffffff</property>
        <property name="activeforeground">#ffffff</property>
        <property name="background">#ff0000</property>
        <property name="command" type="command" cbtype="simple">muxoff</property>
        <property name="font">{Arial} 9 {bold}</property>
        <property name="text" translatable="yes">OFF</property>
        <property name="textvariable">string:off</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relx">0.65</property>
          <property name="rely">0.71</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Label" id="header" named="True">
        <property name="background">#9dceff</property>
        <property name="font">{Arial} 14 {bold}</property>
        <property name="text" translatable="yes">MULTIPLEXER SWITCH CONTROL</property>
        <layout manager="place">
          <property name="anchor">center</property>
          <property name="relx">0.47</property>
          <property name="rely">0.07</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Text" id="instruction1" named="True">
        <property name="background">#9dceff</property>
        <property name="borderwidth">0</property>
        <property name="font">{Arial} 10 {}</property>
        <property name="height">4</property>
        <property name="text" translatable="yes">Select other and enter COM port value manually when it  is not available in list.         Example: "com3"</property>
        <property name="width">20</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relheight">0.21</property>
          <property name="relwidth">0.35</property>
          <property name="relx">0.42</property>
          <property name="rely">0.17</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
